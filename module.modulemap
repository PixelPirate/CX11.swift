module CX11 [system] {
  module Xlib {
	header "/usr/include/X11/Xlib.h"
  }
  module X {
	header "/usr/include/X11/X.h"
  }
  module Xatom {
      header "/usr/include/X11/Xatom.h"
  }
  module Keysym {
      header "/usr/include/X11/keysym.h"
  }
  module Xf86vmode {
      header "/usr/inlcude/X11/extensions/xf86vmode.h"
  }
  link "X11"
}
